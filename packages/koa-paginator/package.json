{
	"name": "koa-paginator",
	"version": "0.1.1",
	"description": "Koa middleware for processing pagination.",
	"main": "src/index.gen.cjs",
	"type": "module",
	"types": "src/index.d.ts",
	"exports": {
		"import": "./src/index.mjs",
		"require": "./src/index.gen.cjs"
	},
	"scripts": {
		"build": "rollup -c script/rollup.config.mjs",
		"test": "mocha \"**/*.spec.mjs\" --no-diff -t 999999999"
	},
	"files": [
		"src",
		"tsconfig.json"
	],
	"keywords": [
		"koa",
		"paginator",
		"page",
		"middleware"
	],
	"author": "ChaosLee",
	"license": "MIT",
	"devDependencies": {
		"@types/koa": "^2.13.10",
		"@types/mocha": "^10.0.3",
		"@types/node": "^20.8.7",
		"koa": "^2.14.2",
		"mocha": "^10.2.0",
		"rollup": "^4.1.4"
	}
}
